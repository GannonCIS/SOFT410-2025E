#!/usr/bin/env sh
##############################################################################
# Gradle start up script for UN*X
##############################################################################

APP_NAME=Gradle
PROGNAME=`basename "$0"`

# Add default JVM options here. You can also use JAVA_OPTS and GRADLE_OPTS to pass JVM options to this script.
DEFAULT_JVM_OPTS="-Xmx64m"

CLASSPATH="$0"

# Resolve links - $0 may be a link
PRG="$0"
while [ -h "$PRG" ] ; do
  ls=`ls -ld "$PRG"`
  link=`expr "$ls" : '.*-> \(.*\)$'`
  if expr "$link" : '/.*' > /dev/null; then
    PRG="$link"
  else
    PRG=`dirname "$PRG"`"/"`expr "$link" : '\(.*\)$'`
  fi
done

PRGDIR=`dirname "$PRG"`

exec java ${JAVA_OPTS:-$DEFAULT_JVM_OPTS} -cp "$PRGDIR/gradle/wrapper/gradle-wrapper.jar" org.gradle.wrapper.GradleWrapperMain "$@"
